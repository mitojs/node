{
	"name": "@mitojs/node-cli",
	"version": "0.0.1",
	"description": "@mitojs/node-cli",
	"author": "cjinhuo",
	"license": "MIT",
	"bin": {
		"mito-node": "dist/index.js"
	},
	"main": "src/index.ts",
	"module": "src/index.ts",
	"type": "module",
	"types": "src/index.ts",
	"publishConfig": {
		"main": "dist/index.js",
		"module": "dist/index.esm.js",
		"types": "dist/index.d.ts"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"esm": "tsc -b ./tsconfig.json",
		"watch:esm": "tsc -b ./tsconfig.json --watch",
		"rollup": "batch-rollup -d ./rollups",
		"build": "run-s esm rollup",
		"build:cjs": "batch-rollup -d ./rollups -f cjs",
		"lint": "eslint ./src/**",
		"test": "jest --config jest.config.cjs"
	},
	"dependencies": {
		"@mono/rollup": "workspace:*",
		"@types/ink": "2.0.3",
		"@types/react": "19.0.8",
		"commander": "^14.0.0",
		"ink": "6.0.1",
		"react": "19.1.0",
		"uuid": "^11.1.0",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@mono/tsconfig": "workspace:*",
		"@types/ws": "^8.18.1"
	}
}
